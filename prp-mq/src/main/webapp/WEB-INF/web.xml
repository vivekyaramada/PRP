<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>PRP MQ Web App</display-name>
  <listener>
    <listener-class>gov.hhs.cms.prp.servlet.AppContextListener</listener-class>
  </listener>
  <servlet>
    <servlet-name>MessagingTestServlet</servlet-name>
    <servlet-class>gov.hhs.cms.prp.servlet.MessagingTestServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MessagingTestServlet</servlet-name>
    <url-pattern>*.java</url-pattern>
  </servlet-mapping>
</web-app>
