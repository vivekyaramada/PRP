<file-format>
    <converter name="char" class="com.blackbear.flatworm.converters.CoreConverters" method="convertChar"
               return-type="java.lang.String"/>
    <converter name="integer" class="com.blackbear.flatworm.converters.CoreConverters" method="convertInteger"
               return-type="java.lang.Integer"/>
    <converter name="date" class="com.blackbear.flatworm.converters.CoreConverters" method="convertDate"
               return-type="java.util.Date" />

    <!-- BEGIN RECORD DEFINITIONS FOR RETLIST MESSAGE FORMAT -->
    <!-- Record definition for RETLIST message header segment -->
    <record name="retireeListHeader">
        <record-ident>
            <field-ident field-start="0" field-length="4">
                <match-string>RETH</match-string>
            </field-ident>
        </record-ident>
        <record-definition>
            <bean name="retireeListHeader" class="gov.hhs.cms.prp.mq.to.RetireeListHeaderTrailerTO"/>
            <line>
                <record-element length="4" beanref="retireeListHeader.segmentId" type="char"/>
                <record-element length="1" beanref="retireeListHeader.transMethod" type="char"/>
                <record-element length="255" beanref="retireeListHeader.fileName" type="char">
                    <conversion-option name="pad-character" value=" "/>
                    <conversion-option name="justify" value="left"/>
                </record-element>
                <record-element length="8" beanref="retireeListHeader.numberOfRecords" type="integer"/>
                <record-element length="27"/> <!-- Filler -->
            </line>
        </record-definition>
    </record>
    <!-- End record definition for RETLIST message header segment -->

    <!-- Record definition for RETLIST message detail segment -->
    <record name="retireeListDetail">
        <record-ident>
            <field-ident field-start="0" field-length="4">
                <match-string>RLST</match-string>
            </field-ident>
        </record-ident>
        <record-definition>
            <bean name="retireeListDetail" class="gov.hhs.cms.prp.mq.to.RetireeListDetailTO"/>
            <line>
                <record-element length="4" beanref="retireeListDetail.segmentId" type="char"/>
                <record-element length="10" beanref="retireeListDetail.applicationId" type="integer">
                    <conversion-option name="pad-character" value="0"/>
                    <conversion-option name="justify" value="right"/>
                </record-element>
                <record-element length="20" beanref="retireeListDetail.rxGroupNumber" type="char">
                    <conversion-option name="pad-character" value=" "/>
                    <conversion-option name="justify" value="left"/>
                </record-element>
                <record-element length="8" beanref="retireeListDetail.coverageEffectiveDate" type="date"/>
                <record-element length="8" beanref="retireeListDetail.coverageTerminationDate" type="date"/>
                <record-element length="30" beanref="retireeListDetail.beneFirstName" type="char">
                    <conversion-option name="pad-character" value=" "/>
                    <conversion-option name="justify" value="left"/>
                </record-element>
                <record-element length="1" beanref="retireeListDetail.beneMiddleInitial" type="char"/>
                <record-element length="40" beanref="retireeListDetail.beneLastName" type="char">
                    <conversion-option name="pad-character" value=" "/>
                    <conversion-option name="justify" value="left"/>
                </record-element>
                <record-element length="9" beanref="retireeListDetail.beneSsn" type="char"/>
                <record-element length="12" beanref="retireeListDetail.beneHicn" type="char" />
                <record-element length="8" beanref="retireeListDetail.beneBirthDate" type="date" />
                <record-element length="1" beanref="retireeListDetail.beneSexCode" type="char" />
                <record-element length="2" beanref="retireeListDetail.beneRelationshipCode" type="char" />
                <record-element length="3" beanref="retireeListDetail.recordActionCode" type="char" />
                <record-element length="8" beanref="retireeListDetail.fileReceiptDate" type="date" />
                <record-element length="10" beanref="retireeListDetail.planId" type="integer">
                    <conversion-option name="pad-character" value="0"/>
                    <conversion-option name="justify" value="right"/>
                </record-element>
                <record-element length="6"/> <!-- Filler -->
            </line>
        </record-definition>
    </record>
    <!-- End record definition for RETLIST message detail segment -->

    <!-- Record definition for RETLIST message trailer segment -->
    <record name="retireeListTrailer">
        <record-ident>
            <field-ident field-start="0" field-length="4">
                <match-string>RETT</match-string>
            </field-ident>
